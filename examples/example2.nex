# GUI Application Example in Nex

putPy tkinter

func main() do
    var root = tkinter.Tk()
    root.title("Nex GUI Example")

    func greet() do
        log("Hello from Nex GUI!")
    end

    var button = tkinter.Button(root, text="Click Me", command=greet)
    button.pack()

    root.mainloop()
end

main()
